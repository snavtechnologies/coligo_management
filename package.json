  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "client": "ng serve --host 0.0.0.0",
    "server": "nodemon backend/server.js",
    "cm": "cd src/app/menu_components && ng g m $m --routing --spec false && ng g c $m --spec false",
    "csm": "cd src/app/menu_components && ng g m $m --routing --spec false",
    "csmi": "cd src/app/menu_components/$m && ng g c $c --spec false",
    "soft_create_specific_windows": "soft_create.bat",
    "soft_create_all_windows": "sequelize-auto -o backend/models/models_reference -d nace_fin_core -h localhost -u root -p 3306 -e mysql -a backend/models/json_models/all_table_prop.json",
    "soft_create_all": "sequelize-auto -o './backend/models/models_reference' -d nace_fin_core -h localhost -u root -p 3306 -x root -e mysql -a './backend/models/json_models/all_table_prop.json'",
    "soft_create_specific": "sequelize-auto -o './backend/models/models_reference' -d nace_fin_core -h localhost -u root -p 3306 -x root -e mysql -t $m -a './backend/models/json_models/all_table_prop.json'",
    "kill_angular": "sudo fuser -k 4200/tcp",
    "kill_node": "sudo fuser -k 3001/tcp"
  },
